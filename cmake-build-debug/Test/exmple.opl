def sqrt(x: float) {
    if (x == 0.0 || x == 1.0) {
        return x;
    }
    let guess: float = x / 2.0;
    while (true) {
        let new_guess: float = (guess + x / guess) / 2.0;
        if (new_guess == guess || (new_guess - guess < 0.000001 && new_guess - guess > -0.000001)) {
            return new_guess;
        }
        guess = new_guess;
    }
}

def main() {
	println(sqrt(4));
}